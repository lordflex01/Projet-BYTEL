<!DOCTYPE html>

<title>Imput index</title>

{% block body %}
    <h1>Imput index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Valeur</th>
                <th>Date</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for imput in imputs %}
            <tr>
                <td>{{ imput.id }}</td>
                <td>{{ imput.valeur }}</td>
                <td>{{ imput.date ? imput.date|date('Y-m-d') : '' }}</td>
                <td>
                    <a href="{{ path('imput_show', {'id': imput.id}) }}">show</a>
                    <a href="{{ path('imput_edit', {'id': imput.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('imput_new') }}">Create new</a>
{% endblock %}
